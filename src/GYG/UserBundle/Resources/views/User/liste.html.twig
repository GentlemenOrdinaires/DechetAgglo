<table>
    <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Gestion</th>
    </tr>
{% for user in users %}
        <tr>
            <td>{{ user.username }}</td>
            <td>{{ user.email }}</td>
            <td>
                <a href="{{ path('gyg_user_delete', {'idUser': user.id}) }}">Delete</a>
            </td>
        </tr>
{% endfor %}
</table>

